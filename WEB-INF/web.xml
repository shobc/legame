<web-app>
    <filter>
        <filter-name>loginCheckFilter</filter-name>
        <filter-class>filter.LoginCheckFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>loginCheckFilter</filter-name>
        <url-pattern>/profile</url-pattern>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>

    <filter>
        <filter-name>authFilter</filter-name>
        <filter-class>filter.AuthFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>authFilter</filter-name>
        <url-pattern>/profile</url-pattern>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>

    <servlet>
        <servlet-name>loginservlet</servlet-name>
        <servlet-class>LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginservlet</servlet-name>
        <url-pattern>/loginservlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>loginPage</servlet-name>
        <servlet-class>LoginPageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginPage</servlet-name>
        <url-pattern>/loginPage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FriendSearchServlet</servlet-name>
        <servlet-class>FriendSearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FriendSearchServlet</servlet-name>
        <url-pattern>/FriendSearchServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FriendAddServlet</servlet-name>
        <servlet-class>FriendAddServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FriendAddServlet</servlet-name>
        <url-pattern>/FriendAddServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>profilePage</servlet-name>
        <servlet-class>ProfilePageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>profilePage</servlet-name>
        <url-pattern>/profilePage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterAccountServlet</servlet-name>
        <servlet-class>RegisterAccountServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterAccountServlet</servlet-name>
        <url-pattern>/RegisterAccountServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterProfileServlet</servlet-name>
        <servlet-class>RegisterProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterProfileServlet</servlet-name>
        <url-pattern>/RegisterProfileServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>deletesession</servlet-name>
        <servlet-class>DSession</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deletesession</servlet-name>
        <url-pattern>/deletesession</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>testpage</servlet-name>
        <jsp-file>/testpage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>testpage</servlet-name>
        <url-pattern>/testpage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>login</servlet-name>
        <jsp-file>/login.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>profile</servlet-name>
        <jsp-file>/profile.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>profile</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>registerProfile</servlet-name>
        <jsp-file>/RegisterProfile.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerProfile</servlet-name>
        <url-pattern>/registerProfile</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registerAccount</servlet-name>
        <jsp-file>/RegisterAccount.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>registerAccount</servlet-name>
        <url-pattern>/registerAccount</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>registeredProfilePage</servlet-name>
        <jsp-file>/registeredProfilePage.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>registeredProfilePage</servlet-name>
        <url-pattern>/registeredProfilePage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>friendSearch</servlet-name>
        <jsp-file>/friendSearch.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>friendSearch</servlet-name>
        <url-pattern>/friendSearch</url-pattern>
    </servlet-mapping>
</web-app>